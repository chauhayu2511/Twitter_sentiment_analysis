        "# Create network plot by using the function networkx, to visualize the \n",
        "  # network relatinoship between the words\n",
        "G = nx.Graph()\n",
        "\n",
        "# Create connections between nodes\n",
        "for k, v in d[0].items():\n",
        "    G.add_edge(k[0], k[1], weight=(v * 10))\n",
        "\n",
        "G.add_node(\"FIFAWorldcup\", weight=100)"
      ]
